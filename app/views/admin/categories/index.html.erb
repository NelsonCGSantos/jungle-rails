 <header class="page-header">
    <h1>Categories</h1>
  </header>

<div class="categories-ind">

<table>
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
    <% @categories.each do |category| %>
      <tr>
        <td><%= category.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'New Category', new_admin_category_path, class: 'btn btn-primary' %>
</div>
